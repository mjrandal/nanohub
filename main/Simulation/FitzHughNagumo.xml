            <!-- <Gap>10</Gap> -->
            <!-- <Width>5</Width> -->
            <!-- <Types>SecretingCells</Types> -->
        <!-- </Region> -->
        <Region>
            <BoxMin x="0" y="0" z="0"/>
            <BoxMax x="128" y="128" z="1"/>
            <Gap>10</Gap>
            <Width>1</Width>
            <Types>SecretingCells,NonSecretingCells</Types>
        </Region>
        <Region>
            <BoxMin x="0" y="60" z="0"/>
            <BoxMax x="40" y="65" z="1"/>
